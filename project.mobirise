        {
          "alias": "amp-blocks/components",
          "_isSecondary": true,
          "typeCompatible": "isAMP",
          "_styles": {
            "padding-top": "(@paddingTop * 1rem)",
            "padding-bottom": "(@paddingBottom * 1rem)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".card-wrapper": {
              ".card": {
                "padding-left": "0",
                "padding-right": "0",
                ".img": {
                  "width": "100%",
                  "amp-img img": {
                    "object-fit": "cover"
                  }
                },
                ".card-text": {
                  "width": "100%",
                  "text-align": "center",
                  "& when (@showBorderTitleCard)": {
                    ".card-subtitle": {
                      "padding-bottom": "4px",
                      "&:after": {
                        "content": "\"\"",
                        "width": "30%",
                        "height": "2px",
                        "display": "block",
                        "margin": "auto",
                        "margin-top": "0.5rem",
                        "background-color": "@colorBorderTitleCard"
                      }
                    }
                  },
                  "& when (@transparentBg)": {
                    "background": "transparent"
                  },
                  "& when not (@transparentBg)": {
                    "background": "@cardBg"
                  }
                }
              }
            },
            ".line .line-line-color": {
              "border-bottom-color": "@colorBorderTitle"
            },
            "& when (@borderCards)": {
              ".card": {
                "& when not (@transparentBg)": {
                  "border": "1px solid darken(@cardBg, 13.5%)"
                },
                "& when (@transparentBg)": {
                  "border": "1px solid fade(#000000, 13.5%)"
                }
              }
            },
            "H3": {
              "color": "#ffffff"
            },
            ".card-title, .card-btn, .card-img": {
              "color": "#ffffff"
            },
            ".card-subtitle, .card-btn, .card-img": {
              "color": "#efefef"
            }
          },
