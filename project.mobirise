        {
          "alias": "amp-blocks/components",
          "_isSecondary": true,
          "typeCompatible": "isAMP",
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".card-img": {
              "max-height": "660px",
              "amp-img": {
                "max-height": "630px"
              }
            },
            ".facebook-wrap when (@showfbLike)": {
              "width": "100px",
              "height": "20px"
            },
            ".card-title-box when (@showCardsInfo), (@showCardsCategory), (@showCardsDate), (@showfbLike)": {
              "justify-content": "space-between",
              ".category-wrap > *": {
                "display": "inline-block",
                "padding-right": "14px"
              },
              ".category-wrap when (@showLinePost)": {
                "& when (@showCardsCategory), (@showCardsDate)": {
                  ".card-category, .card-date": {
                    "&:before": {
                      "content": "''",
                      "display": "inline-block",
                      "height": "0",
                      "width": "0",
                      "background-color": "@warningColor"
                    },
                    "& when (@showEffectLinePost)": {
                      "&:before": {
                        "-webkit-transition": "all .2s",
                        "transition": "all .2s"
                      },
                      "&:hover:before": {
                        "transform": "rotate(90deg)",
                        "background-color": "@primaryColor"
                      }
                    }
                  },
                  ".card-category:before when (@showCardsInfo)": {
                    "height": "12px",
                    "width": "1px",
                    "margin-right": "14px"
                  },
                  ".card-date:before when (@showCardsCategory), (@showCardsInfo)": {
                    "height": "12px",
                    "width": "1px",
                    "margin-right": "14px"
                  }
                }
              }
            },
            ".mbr-col-xl-4 when (@milCount = 3) and (@fullWidth)": {
              "@media (min-width: 1200px)": {
                "flex-basis": "33.3%",
                "max-width": "33.3%"
              }
            },
            "@media (min-width: 768px)": {
              "& when (@showLineText)": {
                "& when (@showCardsTitle), (@showCardsSubtitle), (@showCardsText), (@showCardsButtons)": {
                  ".card-text-box, .card-title-box": {
                    "padding-left": "75px"
                  },
                  ".card-text-box": {
                    "position": "relative",
                    "&:before": {
                      "content": "\"\"",
                      "position": "absolute",
                      "top": "14px",
                      "left": "0",
                      "width": "45px",
                      "height": "3px",
                      "background-color": "@primaryColor"
                    }
                  },
                  "@media (min-width: 992px)": {
                    ".card-text-box, .card-title-box": {
                      "padding-left": "100px"
                    },
                    ".card-text-box:before": {
                      "width": "70px"
                    }
                  }
                }
              }
            },
            ".title when (@showMilestone) and not (@titleAlign = '2')": {
              ".milestone, .milestone > div, .milestone > div > div": {
                "width": "100%"
              },
              ".milestone > div > div when (@showBorderBottom)": {
                "border-bottom": "1px solid @milBorderColor"
              },
              ".milestone .mbr-text": {
                "padding-top": "5px"
              },
              ".milestone > div when (@showTitle), (@showSubtitle)": {
                "& when (@milCount > 2)": {
                  "&:nth-child(1), &:nth-child(2)": {
                    "padding-bottom": "20px"
                  }
                }
              },
              "@media (max-width: 767px)": {
                ".title-wrap": {
                  "padding-bottom": "2rem"
                },
                ".milestone > div:not(:last-child)": {
                  "padding-bottom": "20px"
                },
                ".milestone .numeral": {
                  "justify-content": "center"
                }
              }
            },
            ".card-wrapper, .card-wrapper > *:not(.mbr-overlay)": {
              "position": "relative"
            },
            ".card-wrapper .mbr-overlay when not (@transparentBg)": {
              "background": "@cardBg",
              "opacity": "@cardOpacity"
            },
            ".card-wrapper when (@showBorders)": {
              "border": "1px solid @bordersColor"
            },
            "@media (max-width: 767px)": {
              ".milestone > div:last-child > div": {
                "border-bottom": "none"
              },
              ".card-title-box .category-wrap": {
                "width": "100%",
                "display": "flex",
                "-webkit-justify-content": "space-between",
                "justify-content": "space-between",
                "padding-bottom": "0.5rem",
                "& > *": {
                  "padding-right": "0"
                }
              }
            }
          },
          "_name": "ampEvents21",
          "_customHTML": "<section class=\"ampEvents21\" group=\"Events\" plugins=\"amp-main, amp-facebook-like\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n            <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n            <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n            <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <header>Title</header>\n            <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n            <input type=\"checkbox\" title=\"Milestone With Number\" name=\"showMilestone\" checked=\"\">\n                <input type=\"checkbox\" title=\"Border Bottom\" name=\"showBorderBottom\" condition=\"showMilestone\" checked=\"\">\n                <input type=\"color\" title=\"Border Color\" name=\"milBorderColor\" value=\"#bec1c7\" condition=\"showMilestone &amp;&amp; showBorderBottom\" checked=\"\">\n                <input type=\"range\" inline=\"\" title=\"Count Milestone\" name=\"milCount\" min=\"1\" max=\"4\" step=\"1\" value=\"2\" condition=\"showMilestone\">\n            <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n                <option value=\"1\" selected=\"\">Left</option>\n                <option value=\"2\">Center</option>\n                <option value=\"3\">Right</option>\n            </select>\n        \n        <header>Text</header>\n            <input type=\"checkbox\" title=\"Show Post Info\" name=\"showCardsInfo\">\n            <input type=\"checkbox\" title=\"Show Post Category\" name=\"showCardsCategory\">\n            <input type=\"checkbox\" title=\"Show Post Date\" name=\"showCardsDate\">\n            <input type=\"checkbox\" title=\"Show Facebook Like\" name=\"showfbLike\" checked=\"\">\n            <input type=\"text\" title=\"Facebook Like URL\" name=\"fbLinkLike\" value=\"https://www.facebook.com/completebathroomsgroup\" condition=\"showfbLike\">\n            <input type=\"checkbox\" title=\"Show line Info Post\" name=\"showLinePost\" condition=\"(showCardsInfo &amp;&amp; showCardsCategory) || (showCardsInfo &amp;&amp; showCardsDate) || (showCardsCategory &amp;&amp; showCardsDate)\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Effect Post\" name=\"showEffectLinePost\" condition=\"showLinePost &amp;&amp; ((showCardsInfo &amp;&amp; showCardsCategory) || (showCardsInfo &amp;&amp; showCardsDate) || (showCardsCategory &amp;&amp; showCardsDate))\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Title\" name=\"showCardsTitle\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showCardsSubtitle\">\n            <input type=\"checkbox\" title=\"Show Text\" name=\"showCardsText\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Buttons\" name=\"showCardsButtons\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show line Text\" name=\"showLineText\" condition=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons\">\n            <select title=\"Align\" name=\"cardsAlign\" condition=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons\">\n                <option value=\"1\" selected=\"\">Left</option>\n                <option value=\"2\">Center</option>\n                <option value=\"3\">Right</option>\n            </select>\n            <input type=\"checkbox\" title=\"Borders\" name=\"showBorders\" condition=\"showCardsInfo || showCardsCategory || showCardsDate || showfbLike || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtonsg\">\n            <input type=\"color\" title=\"Color\" name=\"bordersColor\" value=\"#cccccc\" condition=\"showBorders &amp;&amp; (showCardsInfo || showCardsCategory || showCardsDate || showfbLike || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons)\">\n            <input type=\"checkbox\" title=\"Transparent BG\" name=\"transparentBg\" condition=\"showCardsInfo || showCardsCategory || showCardsDate || showfbLike || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons\" checked=\"\">\n            <input type=\"color\" title=\"Color\" name=\"cardBg\" value=\"#bec1c7\" condition=\"!transparentBg &amp;&amp; (showCardsInfo || showCardsCategory || showCardsDate || showfbLike || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons)\">\n            <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"cardOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.3\" condition=\"!transparentBg &amp;&amp; (showCardsInfo || showCardsCategory || showCardsDate || showfbLike || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons)\">\n        <header>Image</header>\n            <input type=\"checkbox\" title=\"Show Image\" name=\"showImg\" checked=\"\">\n        \n        <header>Background</header>\n            <input type=\"checkbox\" title=\"2 Background Color\" name=\"twoBgColor\" condition=\"bg.type == 'color'\"> \n            <input type=\"range\" inline=\"\" title=\"Width 2 Bg\" name=\"width2Bg\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">    \n            <fieldset type=\"background\" name=\"bg\">\n                <input type=\"image\" title=\"Background Image\" value=\"../_images/background1.jpg\">\n                <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected=\"\">\n            </fieldset>\n            <input type=\"color\" title=\"2 Color\" name=\"twoColor\" value=\"#f4f5f7\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">  \n            <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n            <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n            <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type !== 'color') || (twoBgColor &amp;&amp; bg.type == 'color')\" mbr-class=\"{'mbr-col-md-4': width2Bg == 1 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-5': width2Bg == 2 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-6': width2Bg == 3 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-7': width2Bg == 4 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-8': width2Bg == 5 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color'}\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n                     'container-fluid':fullWidth}\">\n        <div class=\"mbr-row title mbr-pb-4\" mbr-if=\"showTitle || showSubtitle || showMilestone\" mbr-class=\"{'mbr-row-reverse':showMilestone &amp;&amp; (showTitle || showSubtitle) &amp;&amp; titleAlign != 2 &amp;&amp; reverseContentTitle, 'mbr-px-2': fullWidth, 'align-left': titleAlign == 1, 'align-center': titleAlign == 2, 'align-right': titleAlign == 3}\">\n            <div class=\"title-wrap\" mbr-class=\"{'mbr-col-sm-12 mbr-col-md-4': showMilestone &amp;&amp; titleAlign != 2, 'mbr-col-sm-12 mbr-col-md-12':!showMilestone || titleAlign == 2}\" mbr-if=\"showTitle || showSubtitle\">        \n                <h2 class=\"mbr-section-title mbr-semibold mbr-fonts-style\" mbr-class=\"{'mbr-pb-2':showSubtitle}\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".title-wrap .mbr-section-title\">\n    